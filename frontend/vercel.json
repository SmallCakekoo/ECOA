{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "cd frontend && npm install",
  "rewrites": [
    {
      "source": "/",
      "destination": "/client-Home.html"
    },
    {
      "source": "/client/:path*",
      "destination": "/client-:path*.html"
    },
    {
      "source": "/admin/:path*",
      "destination": "/admin-:path*.html"
    }
  ],
  "cleanUrls": true
}